            "items": {
              "type": "string"
            }
          },
          "currentPage": {
            "type": "number",
            "example": 1,
            "description": "Current page number."
          },
          "totalElements": {
            "type": "number",
            "example": 100,
            "description": "Total number of elements."
          },
          "nextPage": {
            "type": "number",
            "example": 2,
            "description": "Next page number, or null if last page."
          },
          "lastPage": {
            "type": "number",
            "example": 10,
            "description": "Last page number."
          },
          "firstPage": {
            "type": "number",
